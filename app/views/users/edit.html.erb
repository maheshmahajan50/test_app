<%= form_with model: @user, url: user_path(@user), method: :patch, local: true do |f| %>

  <%= f.text_field :first_name %>
  <%= f.text_field :last_name %>
  <%= f.email_field :email %>
  <%= f.select(:insurance_ids, Insurance.all.collect {|i| [ i.title, i.id ] }, 
                                           { :prompt => "Please select"}, 
                                           { :multiple => true }) %>
  <%= f.submit %>

<% end %>